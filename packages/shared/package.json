{
  "name": "@promptrepo/shared",
  "version": "0.0.1",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./env": {
      "types": "./dist/env.d.ts",
      "import": "./dist/env.mjs",
      "require": "./dist/env.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/env.ts --format cjs,esm --dts --external @supabase/supabase-js --external @upstash/redis --external stripe",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.4",
    "@upstash/redis": "^1.25.1",
    "stripe": "^14.9.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@promptrepo/config": "workspace:*",
    "@types/node": "^20.10.0",
    "eslint": "^8.55.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "vitest": "^1.0.0"
  }
}